  <!-- Sentry Configuration -->
  <appSettings>
    <!-- Replace with your actual Sentry DSN -->
    <add key="SentryDsn" value="YOUR_SENTRY_DSN_HERE" />
    
    <!-- Environment: Development, Staging, Production -->
    <add key="SentryEnvironment" value="Development" />
    
    <!-- Release version -->
    <add key="SentryRelease" value="UpdateServer@1.0.0" />
    
    <!-- Debug mode: true/false -->
    <add key="SentryDebug" value="false" />
    
    <!-- Performance monitoring sample rate: 0.0 to 1.0 -->
    <add key="SentryTracesSampleRate" value="1.0" />
  </appSettings>
  
